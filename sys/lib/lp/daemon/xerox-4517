#!/bin/rc
# The destination is set to 'ps' for the Xerox printer
generic 'aux/download -f -H/sys/lib/postscript/font -mfontmap -plw+ < $FILE | aux/lpdsend -dPASSTHRU -H$LPMACHID -P$LPUSERID $OUTDEV >[2] $PRINTLOG' '{cat <{echo -d$LPDEST -pnoproc -M$SCHEDLINE(1) -u$SCHEDLINE(2)} $LPDEST/$FILE(1);sleep 5} | lpsend.rc $DEST_HOST' ''
