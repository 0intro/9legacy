#!/bin/rc

sed '
	s/[ 	]+$//
	/	/!d
	s/\\N''349''//g
	s/''//g
	s/	-/	/
	s/-$//
	/\([^,) ]+(\)|$)/{; h; s///; p; g; s/\(//; s/\)//; }
	/\(r, s\)$/{; s///; p; s/$/r/; p; s/r$/s/; }
' $1 |
sed '
	/\([^,) ]+(\)|$)/{; h; s///; p; g; s/\(//; s/\)//; }
	/\(r, s\)$/{; s///; p; s/$/r/; p; s/r$/s/; }
' |
sed '/ß/{; p; s/ß/ss/g; }' |
awk '
BEGIN	{ FS = "	|, " }
	{ for(i=2; i<=NF; i++)print $i "	" $1 }
' |
tr A-Z a-z |
sort -u -t'	' +0f -1 +0 -1 +1n -2
