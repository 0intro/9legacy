</$objtype/mkfile

TARG=sam
OFILES=sam.$O\
	address.$O\
	buff.$O\
	cmd.$O\
	disk.$O\
	error.$O\
	file.$O\
	io.$O\
	list.$O\
	mesg.$O\
	moveto.$O\
	multi.$O\
	plan9.$O\
	rasp.$O\
	regexp.$O\
	shell.$O\
	string.$O\
	sys.$O\
	util.$O\
	xec.$O\

HFILES=sam.h\
	errors.h\
	mesg.h\

BIN=/$objtype/bin

UPDATE=\
	mkfile\
	parse.h\
	$HFILES\
	${OFILES:%.$O=%.c}\

</sys/src/cmd/mkone

address.$O cmd.$O parse.$O xec.$O unix.$O:	parse.h

